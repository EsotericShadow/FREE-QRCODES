{
  "version": 2,
  "builds": [
    {
      "src": "api/qrcode.py",
      "use": "@vercel/python"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "script.js",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/qrcode.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/$",
      "dest": "/index.html"
    }
  ]
}
